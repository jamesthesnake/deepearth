{
  "visualization": {
    "title": "Plant Species Embeddings",
    "subtitle": "Florida Plant Species visualized using DeepSeek-V3 embeddings",
    "defaultSphereSize": 0.05,
    "defaultTextSize": 0.3,
    "scaleFactor": 3.0
  },
  "data": {
    "sourceFile": "path/to/your/embeddings.json",
    "fields": {
      "id": "id",
      "label": "name",
      "cluster": "cluster",
      "metadata": ["color"]
    }
  },
  "images": {
    "enabled": true,
    "sourcePath": "static/images",
    "summaryFile": "path/to/your/image_summary.json",
    "urlPattern": "/static/images/{directory}/",
    "summaryKeyField": "name",
    "summaryImageField": "taxon_id",
    "directoryPattern": "taxon_{taxon_id}",
    "filePattern": "*_img_{index}.jpg",
    "maxImages": 999
  },
  "server": {
    "host": "localhost",
    "port": 8080,
    "openBrowser": true
  }
}